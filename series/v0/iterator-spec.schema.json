{
  "$id": "https://goodlittlescript.github.io/series/v0/iterator-spec.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "An iteration series: n1 = n0 + step",
  "type": "object",
  "properties": {
    "start": {
      "type": "number",
      "default" : 0
    },
    "step": {
      "type": "number",
      "default" : 1
    },
    "num": {
      "type": "number",
      "default" : 10
    }
  },
  "additionalProperties": false
}
