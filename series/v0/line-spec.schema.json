{
  "$id": "https://goodlittlescript.github.io/series/v0/line-spec.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A line series: f(x) = mx + b",
  "type": "object",
  "properties": {
    "m": {
      "type": "number",
      "default" : 1
    },
    "b": {
      "type": "number",
      "default" : 0
    },
    "x": {
      "$ref": "https://goodlittlescript.github.io/series/v0/step.schema.json"
    }
  },
  "additionalProperties": false
}
